#include<stdio.h>

void swap(int *ap, int *bp);

int main() {
	int a = 3, b = 5;
	int *ap, *bp;
	ap = &a;
	bp = &b;
	printf("a = %d\n", a);
	printf("b = %d\n", b);
	swap(ap, bp);
	printf("a = %d\n", a);
	printf("b = %d\n", b);


}


void swap(int *ap, int *bp) {
	int tmp;
	tmp = *ap;
	*ap = *bp;
	*bp = tmp;
}
