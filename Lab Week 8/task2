#include<stdio.h>

void swap(int *a, int *b);

int main() {
	int a = 3, b = 5;
	printf("a = %d\n", a);
	printf("b = %d\n", b);
	swap(&a, &b);
	printf("a = %d\n", a);
	printf("b = %d\n", b);


}


void swap(int *a, int *b) {
	int tmp;
	tmp = *a;
	*a = *b;
	*b = tmp;
}
